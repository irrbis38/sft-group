{% from "components/card/news/news.njk" import newsCard %}

<div class="news-slider swiper">
    <div class="news-slider__head">
        <h2 class="news-slider__title h2">Новости</h2>
        <div class="swiper-buttons-static">
            <div class="swiper-button-default swiper-button-prev"></div>
            <div class="swiper-button-default swiper-button-next"></div>
        </div>
    </div>
    <div class="swiper-wrapper">
        {% for item in range(9) %}
            <div class="swiper-slide">
                {{ newsCard() }}
            </div>
        {% endfor %}
    </div>
</div>